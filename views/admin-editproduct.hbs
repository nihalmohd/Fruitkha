{{>adminHeader}}

	<!-- check out section -->
	<div class="checkout-section mt-100 mb-150">
		<div class="container">
			<div class="row">
				<div class="col-lg-8">
					<div class="checkout-accordion-wrap">
						<div class="accordion" id="accordionExample">
							<div class="card single-accordion">
								<div class="card-header" id="headingOne">
									<h5 class="mb-0">
										<button class="btn btn-link" type="button" data-toggle="collapse"
											data-target="#collapseOne" aria-expanded="true" aria-controls="collapseOne">
											Product Detail
										</button>
									</h5>
								</div>

								<div id="collapseOne" class="collapse show" aria-labelledby="headingOne"
									data-parent="#accordionExample">
									<div class="card-body">
										<div class="billing-address-form">
											{{#editdata}}
											<form onsubmit="return editproductFun()" oninput="admineditproductrrrdisplay()" method="post"
												action="/admin/admin-editproduct/{{_id}}" enctype="multipart/form-data"
												id="editproduct" name="editproduct">
												<p><input name="productname" id="productname" type="text"
														placeholder="Product Name" value="{{productname}}"></p>
												<p class="text-danger text-center"></p>
												<p><input name="brand" type="text" placeholder="Brand"
														value="{{brand}}"></p>
												<p class="text-danger text-center"></p>

												<select class="form-select form-select-lg mb-3 pt-4"
													aria-label=".form-select-lg example" name="catogory" id="catogory">
													<option value="" selected>{{catogory}}</option>
													{{#each ../displaycategory}}
													<option value="{{category}}">{{category}}</option>
													{{/each}}
												</select>
												<p class="text-danger text-center"></p>

												<p><input name="price" type="text" placeholder="Price"
														value="{{price}}"></p>
												<p class="text-danger text-center"></p>

												<p><input name="stoke" type="number" placeholder="Stoke"
														value="{{stoke}}"></p>
												<p class="text-danger text-center"></p>

												<p><input name="image" type="file" placeholder="Image" value="image"
														required></p>
												<p><textarea name="description" id="bill" cols="30" rows="10"
														placeholder="Description"
														value="{{description}}">{{description}}</textarea></p>
												<p class="text-danger text-center"></p>

												<button type="submit" class="btn btn-outline-dark">Update
													product</button>
											</form>
											{{/editdata}}
										</div>
									</div>
								</div>
