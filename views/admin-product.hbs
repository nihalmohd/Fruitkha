    <!-- jquery -->
    <div class="container">
        <div class="row mt-3">
            <div class="col-12">
                <div class="table-responsive p-2">
                    <table class="table table-striped table-bordered" id="example">
                        <thead>
                            <tr>
                                <th>Image</th>
                                <th>Product Name</th>
                                <th>Brand</th>
                                <th>Category</th>
                                <th>Price</th>
                                <th>Stock</th>
                                <th>Action</th>
                            </tr>
                        </thead>
                        <tbody>
                            {{#each product}}
                            <tr>
                                <td><img src="/product-images/{{image.[0]}}" width="50px" height="50px" alt=""></td>
                                <td>{{productname}}</td>
                                <td>{{brand}}</td>
                                <td>{{catogory}}</td>
                                <td>{{price}}</td>
                                <td>{{stoke}}</td>
                                <td>
                                    <a href="/admin/admin-editproduct/{{_id}}" class="btn btn-primary">Edit</a>
                                    {{#if status}}
                                    <a href="/admin/listproduct/{{_id}}">
                                        <button type="button" class="btn btn-danger" data-toggle="modal"
                                            data-target=".bd-example-modal-sm"
                                            onclick="return confirm('Are you sure to unlist the data?')">unlist</button>
                                        {{else}}
                                        <a href="/admin/unlistproduct/{{_id}}">
                                            <button type="button" class="btn btn-danger" data-toggle="modal"
                                                data-target=".bd-example-modal-sm"
                                                onclick="return confirm('Are you sure to list the data?')">list</button>
                                            {{/if}}
                                        </a>
                                        <div class="modal fade bd-example-modal-sm" tabindex="-1" role="dialog"
                                            aria-labelledby="mySmallModalLabel" aria-hidden="true">
                                            <div class="modal-dialog modal-sm">
                                                <div class="modal-content">
                                                    Delete Cancelled
                                                </div>
                                            </div>
                                        </div>
                                </td>
                            </tr>
                            {{/each}}
                        </tbody>
                    </table>
                </div>
            </div>
        </div>
    </div>




